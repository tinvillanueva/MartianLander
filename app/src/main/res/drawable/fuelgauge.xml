<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape>
            <stroke
                android:width="1dp"
                android:color="#d9ff090e"/>
            <gradient
                android:startColor="#ffc9c6c9"
                android:centerColor="#d9ff090e"
                android:endColor="#ffc9c6c9"
                android:centerY="1.0"
                android:angle="45"/>
        </shape>
    </item>

    <item>
        <clip>
            <shape>
                <stroke
                    android:width="1dp"
                    android:color="#ffc9c6c9"/>
                <gradient
                    android:startColor="#ff14cd3f"
                    android:centerColor="#ff14cd3f"
                    android:endColor="#ffc9c6c9"
                    android:centerY="1.0"
                    android:angle="45"/>
            </shape>
        </clip>
    </item>

</layer-list>